<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefWidth="450.0" spacing="5.0" stylesheets="@dictionary.css" xmlns="http://javafx.com/javafx/8.0.331" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.main_app.components.contest.brute_force_input.dictionary_displayer.DictionaryController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Label text="Dictionary">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="filterDictionaryTextField" promptText="Filter Dictionary Words" />
   <ScrollPane fx:id="dictScrollPane" prefHeight="100.0" prefWidth="600.0">
      <content>
         <FlowPane fx:id="wordsFlowPane" alignment="TOP_CENTER" hgap="5.0" vgap="5.0" />
      </content>
   </ScrollPane>
   <HBox spacing="10.0">
      <children>
         <Label text="Excluded Characters: " />
         <Label fx:id="excludedCharsLabel" />
      </children>
   </HBox>
</VBox>
