<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="machineConfigurationComponent" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.331" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.main_app.components.machine.current_machine_conf.MachineConfigurationController">
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="5.0" prefWidth="150.0">
         <children>
            <Label text="Machine">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Configuration">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </FlowPane>
      <Label text="Rotors" />
      <Label fx:id="currentRotorsLabel">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </Label>
      <Label text="Initial Position" />
      <Label fx:id="initialPositionLabel">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </Label>
      <Label text="Current Notch Distance" />
      <Label fx:id="currentNotchDistance">
         <padding>
            <Insets bottom="5.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label text="Reflector" />
      <Label fx:id="currentReflectorLabel">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </Label>
      <Label text="Plug-Board" />
      <Label fx:id="currentPlugBoardLabel">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets left="5.0" top="10.0" />
   </padding>
</VBox>
