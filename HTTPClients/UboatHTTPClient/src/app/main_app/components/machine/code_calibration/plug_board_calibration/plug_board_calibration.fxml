<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="plugboardCalibrationComponent" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.331" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.main_app.components.machine.code_calibration.plug_board_calibration.PlugboardCalibrationController">
   <children>
      <Label text="Plug Board Selection">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="plug1Button" prefWidth="150.0" promptText="Plug 1" />
      <ComboBox fx:id="plug2Button" prefWidth="150.0" promptText="Plug 2" />
      <HBox alignment="CENTER" spacing="15.0">
         <children>
            <Button fx:id="resetPlugsButton" mnemonicParsing="false" onAction="#resetPlugsButtonAction" text="Reset" />
            <Button fx:id="addPlugButton" mnemonicParsing="false" onAction="#addPlugButtonAction" text="Add Plug" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets top="20.0" />
         </padding>
      </HBox>
      <Label text="Plugs Selected:">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="plugsChosenLabel">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
